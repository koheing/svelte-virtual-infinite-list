    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "cypress": "^6.8.0",
    "cypress-svelte-unit-test": "^3.3.1",
    "eslint": "^7.22.0",
